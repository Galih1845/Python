        "angka = int(input('Masukkan angka prima batas teratas:'))\n",
        "print (\"Bilangan prima kurang dari :\",angka)\n",
        "for i in range(2,angka):\n",
        "  if i>1 :\n",
        "    for n in range(2,i):\n",
        "      if(i%n)==0:\n",
        "        break\n",
        "    else :\n",
        "      print(i)"
